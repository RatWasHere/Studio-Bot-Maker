{
  "name": "studiobotmaker",
  "productName": "Studio Bot Maker",
  "main": "main.js",
  "author": "Rat",
  "description": "Create awesome Discord bots!",
  "window": {
    "min_width": 1170,
    "min_height": 790,
    "icon": "icon.png"
  },
  "repository": "https://github.com/RatWasHere/Studio-Bot-Maker",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.ratapp.sbm",
    "publish": "github",
    "win": {
      "certificateFile": "./StudioCertificate.pfx",
      "publish": {
        "provider": "github",
        "owner": "RatWasHere",
        "repo": "Studio-Bot-Maker"
      },
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "uninstallDisplayName": "Uninstall Studio Bot Maker"
    }
  },
  "dependencies": {
    "@aptabase/electron": "^0.2.2",
    "@electron-forge/maker-base": "^6.1.0",
    "@oceanicjs/builders": "^1.1.9",
    "ansi-to-html": "^0.7.2",
    "axios": "^1.4.0",
    "discord-api-types": "^0.37.50",
    "discord-rpc": "^4.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "fs-extra": "^11.1.1",
    "fse": "^4.0.1",
    "node-run-cmd": "^1.0.1",
    "oceanic-collectors": "^1.0.7",
    "oceanic.js": "^1.7.1",
    "pacote": "^15.2.0",
    "request": "^2.88.2",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@electron-forge/publisher-github": "^6.0.5",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder",
    "cert": "electron-builder create-self-signed-cert -p StudioCertificate",
    "pretty": "prettier . --write"
  },
  "version": "3.5.1"
}
