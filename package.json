{
    "name": "studiobotmaker",
    "productName": "Studio Bot Maker",
    "main": "main.js",
    "author": "Rat",
    "description": "Create awesome Discord bots!",
    "window": {
        "min_width": 1170,
        "min_height": 790,
        "icon": "icon.png"
    },
    "repository": "https://github.com/RatWasHere/Studio-Bot-Maker",
    "publish": {
        "provider": "github",
        "releaseType": "release"
    },
    "build": {
        "appId": "com.ratapp.sbm",
        "publish": "github",
        "win": {
            "certificateFile": "./dist/StudioBotCertificate.pfx",
            "publish": {
                "provider": "github",
                "owner": "RatWasHere",
                "repo": "Studio-Bot-Maker"
            },
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "nsis": {
            "oneClick": false,
            "perMachine": false,
            "allowToChangeInstallationDirectory": true,
            "uninstallDisplayName": "Uninstall Studio Bot Maker"
        }
    },
    "dependencies": {
        "@electron-forge/maker-base": "^6.1.0",
        "@oceanicjs/builders": "^1.1.9",
        "ansi-to-html": "^0.7.2",
        "axios": "^1.4.0",
        "discord-api-types": "^0.37.34",
        "discord-rpc": "^4.0.1",
        "electron-squirrel-startup": "^1.0.0",
        "electron-updater": "^5.3.0",
        "fs": "^0.0.1-security",
        "fs-extra": "^11.1.1",
        "fse": "^4.0.1",
        "oceanic-collectors": "^1.0.7",
        "oceanic.js": "^1.7.1",
        "request": "^2.88.2",
        "simple-updater": "^0.0.3",
        "unzipper": "^0.10.11"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.5",
        "@electron-forge/maker-squirrel": "^6.0.5",
        "@electron-forge/maker-zip": "^6.0.5",
        "@electron-forge/publisher-github": "^6.0.5",
        "electron": "^25.2.0",
        "electron-builder": "^23.6.0"
    },
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "release": "electron-builder",
        "cert": "electron-builder create-self-signed-cert -p StudioCertificate"
    },
    "version": "3.0.1"
}
